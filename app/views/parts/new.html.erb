<h1>Create new part</h1>
<br>
<br>
<%  if flash[:message]%>
  <h4 class="error"><%= flash[:message] %></h4>
  <br>
<% end %>
<form action="/parts" method="post">
  <label for="part[name]">Name: </label>
  <input type="text" name="part[name]" id="">
  <br>
  <br>
  <label for="part[description]">Description: </label>
  <input type="text" name="part[description]" id="">
  <br>
  <br>

<br>
<br>
<button type="submit">Create new part</button>
</form>