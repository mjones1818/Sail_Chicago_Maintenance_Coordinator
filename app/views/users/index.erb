<h1>All Members</h1>
<%  if flash[:message]%>
  <br>
  <h4 class="error" ><%= flash[:message] %></h4>
  <br>
<% end %>
<br>
<table>
<tr>
<th>Username</th>
<th>Email</th>
</tr>
  <% @users.each_with_index do |user, index| %>                    
     <tr>
    <td>
      <a href="/users/<%= user.id %>"><%= user.username %></a>
    </td>
    <td>
      <a href="mailto:<%= user.email %>"><%= user.email %></a>
    </td>
    </tr> 
  <% end %>
</table>